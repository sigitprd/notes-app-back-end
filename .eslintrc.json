{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eol-last": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "no-tabs": ["error", {"allowIndentationTabs": true}]
    }
}
